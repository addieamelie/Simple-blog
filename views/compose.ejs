<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="POST">
<div class="form-group"> <label for="">Title</label>
  <input class="form-control" type="text" name="newTitle" 
  placeholder="Type your title..." />
</div>
<div class="form-group">
  <label for="">Post</label>
  <textarea class="form-control" rows="5" cols="30" 
  name="newBody" placeholder="Type your post..." ></textarea>
</div>
  <button class="btn btn-light" type="submit">Publish</button>
</form>

<%- include("partials/footer") -%>
